    </servlet-mapping>

    <servlet>
        <servlet-name>sessionDemo2</servlet-name>
        <servlet-class>com.yaozhou.session.SessionDemo2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sessionDemo2</servlet-name>
